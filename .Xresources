!=================================!
!=> rxvt-unicode terminal stuff <=!
!=================================!
URxvt*.saveLines:    2048
URxvt*.visualBell:   false
URxvt*.urgentOnBell: true

URxvt*.font:         xft:Droid Sans Mono:size=10
URxvt*.boldFont:     xft:Droid Sans Mono:bold:size=10
URxvt*.scrollBar:    false
! Color to fade to
URxvt*.fadeColor:    #000000
! Percentage to fade towards fadeColor
URxvt*.fading:       5

! enabling clickable links:
URxvt*.urlLauncher:    /usr/bin/www-browser
URxvt*.matcher.button: 1

! urxvt color scheme:
URxvt*.background:   #323232
URxvt*.foreground:   #DEDEDE
URxvt*.colorUL:      #86a2b0

! pseudo transparency
URxvt*.transparent:  true
! 0-99 darkens, 101-200 lightens
URxvt*.shading:      80

!==============!
!=> Xft font <=!
!==============!
Xft*.dpi:            96
Xft*.antialias:      true
Xft*.hinting:        full

