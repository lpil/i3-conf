!=================================!
!=> rxvt-unicode terminal stuff <=!
!=================================!
URxvt.saveLines:    2048
URxvt.visualBell:   false
URxvt.urgentOnBell: true

URxvt.font:         xft:Droid Sans Mono:size=10
URxvt.boldFont:     xft:Droid Sans Mono:bold:size=10
URxvt.scrollBar:    false
URxvt.fading:       10

! enabling clickable links:
URxvt.urlLauncher:    /usr/bin/firefox
URxvt.matcher.button: 1 

! urxvt color scheme:
URxvt*background:   #212121
URxvt*foreground:   #DEDEDE
URxvt*colorUL:      #86a2b0

! pseudo transparency
URxvt*transparent:  true
URxvt*shading:      15

!==============!
!=> Xft font <=!
!==============!
Xft*dpi:            96
Xft*antialias:      true
Xft*hinting:        full

